    "compilerOptions": {
        "allowJs": false,
        "module": "commonjs",
        "target": "ESNext",
        "sourceMap": true,
        "moduleResolution": "node",
        "noImplicitAny": true,
        "strictNullChecks": true,
        "resolveJsonModule": true,
        "baseUrl": "./src/",
        "outDir": "dist/",
        "noEmit": true,
        "paths": {
